{
    "compilerOptions": {
        "target": "es2015",
        "module": "commonjs",
        "declaration": true,
        "outDir": "./.out",
        "strict": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "moduleResolution": "node",
        "esModuleInterop": true
    }
}
